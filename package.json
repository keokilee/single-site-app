{
  "name": "fluidity-browser",
  "version": "1.0.0",
  "description": "Browser component of Fluidity.",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:app && npm run build:browser && node scripts/copy_files.js",
    "build:app": "webpack --config config/production/webpack.app.js",
    "build:browser": "BABEL_ENV=production webpack --config config/production/webpack.browser.js",
    "lint": "eslint . --ext .js,.jsx",
    "package": "electron-packager . Fluidity --platform=darwin --arch=x64 --version=0.36.2 --out=dist",
    "start:dev:app": "node scripts/dev_app.js",
    "start:dev:browser": "node scripts/dev_server.js",
    "start": "node scripts/development.js",
    "start:production": "npm run build && NODE_ENV=production electron .",
    "test": "npm run lint && npm run test:app && npm run test:browser",
    "test:app": "BABEL_ENV=test electron-mocha spec/app --compilers js:babel-core/register",
    "test:browser": "BABEL_ENV=test karma start",
    "test:watch": "npm run test:watch:browser",
    "test:watch:browser": "npm run test:browser -- --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keokilee/fluidity-browser.git"
  },
  "author": "George Lee",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.0",
    "electron-prebuilt": "^0.36.2",
    "react": "^0.14.3",
    "react-css-modules": "^3.6.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "redux": "^3.0.5",
    "redux-saga": "^0.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.2",
    "babel-core": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "electron-debug": "^0.5.0",
    "electron-mocha": "^0.8.0",
    "electron-packager": "^5.1.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.1.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.10.0",
    "eslint-plugin-standard": "^1.2.0",
    "expect": "^1.13.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.0",
    "isparta-loader": "^2.0.0",
    "karma": "0.13.19",
    "karma-coverage": "^0.5.3",
    "karma-electron-launcher": "0.1.0",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "proxyquire": "^1.7.3",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-logger": "^2.3.1",
    "rimraf": "^2.5.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
