{
  "name": "single-site-app",
  "version": "1.0.0",
  "description": "Electron application for creating single site applications.",
  "main": "app/index.js",
  "scripts": {
    "lint": "eslint . --ext .js --ext .jsx",
    "start:dev": "node dev_server.js",
    "start": "electron .",
    "build:osx": "electron-packager . SingleSiteApp --platform=darwin --arch=x64 --version=0.34.1 --out=dist",
    "test": "npm run lint && npm run test:app && npm run test:browser",
    "test:app": "BABEL_ENV=test mocha spec/app",
    "test:browser": "BABEL_ENV=test karma start",
    "test:watch:browser": "BABEL_ENV=test karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keokilee/single-site-app.git"
  },
  "author": "George Lee",
  "license": "ISC",
  "dependencies": {
    "electron-prebuilt": "^0.35.0",
    "react": "^0.14.2",
    "react-css-modules": "^3.6.0",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.3.3",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.23.0",
    "electron-debug": "^0.5.0",
    "electron-packager": "^5.1.1",
    "eslint": "^1.9.0",
    "eslint-config-standard": "^4.1.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.9.0",
    "eslint-plugin-standard": "^1.2.0",
    "expect": "^1.13.0",
    "expect.js": "^0.3.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "isparta-instrumenter-loader": "0.2.1",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-electron-launcher": "0.0.4",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.22",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
