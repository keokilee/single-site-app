{
  "name": "fluidity-browser",
  "version": "1.0.0",
  "description": "Browser component of Fluidity.",
  "main": "main.js",
  "scripts": {
    "build:osx": "electron-packager . Fluidity --platform=darwin --arch=x64 --version=0.35.0 --out=dist",
    "build:dev:app": "webpack --config config/development/webpack.app.js",
    "lint": "eslint . --ext .js,.jsx",
    "start:dev:app": "node scripts/dev_app.js",
    "start:dev:browser": "node scripts/dev_server.js",
    "start": "node scripts/development.js",
    "test": "npm run lint && npm run test:app && npm run test:browser",
    "test:app": "BABEL_ENV=test mocha spec/app",
    "test:browser": "BABEL_ENV=test karma start",
    "test:watch": "npm run test:watch:browser",
    "test:watch:browser": "npm run test:browser -- --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keokilee/fluidity-browser.git"
  },
  "author": "George Lee",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.0",
    "electron-prebuilt": "^0.35.0",
    "react": "^0.14.3",
    "react-css-modules": "^3.6.0",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.23.0",
    "electron-debug": "^0.5.0",
    "electron-packager": "^5.1.1",
    "eslint": "^1.10.0",
    "eslint-config-standard": "^4.1.0",
    "eslint-config-standard-react": "^1.2.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.10.0",
    "eslint-plugin-standard": "^1.2.0",
    "expect": "^1.13.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "isparta-instrumenter-loader": "1.0.0",
    "karma": "^0.13.15",
    "karma-coverage": "^0.5.3",
    "karma-electron-launcher": "0.0.5",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "react-addons-test-utils": "^0.14.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "redux-logger": "^2.0.4",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.8",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
